<div id="nametag" class="row ucontainer">
    <% if (user) { %>
        <div class="col s6 m3">
            <span class="valign-wrapper"><i class="material-icons left mb-green-text">portrait</i><%-username%></span>
        </div>
        <div class="col s6">
            <span class="valign-wrapper"><i class="material-icons left mb-green-text">poll</i><%-name%></span>
        </div>
    <% } else { %>
        <div class="col s12"><span>Player 1</span></div>
        <div class="col s12"><span>Rank: Visitor <a href="/register">(uncredited)</a></span></div>
    <% } %>
</div>